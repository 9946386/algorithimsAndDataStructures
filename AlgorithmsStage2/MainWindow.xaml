<Window x:Name="TestWindow" x:Class="AlgorithmsStage2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AlgorithmsStage2"
        mc:Ignorable="d"
        Title="MainWindow" SizeChanged="TestWindow_SizeChanged" Width="800" Height="450" KeyDown="TestWindow_KeyDown">
    <Grid x:Name="TestGrid" Width="800" Height="450"  >
        <Grid.Background>
            <ImageBrush ImageSource="sea.png" Opacity="0.70"/>
        </Grid.Background>
        <Image x:Name="testImage1" HorizontalAlignment="Left" VerticalAlignment="Top" Height="40" Margin="535,107,0,0"  Width="40" Source="Picture1.png" Stretch="Fill" />
        <Image x:Name="testImage2" HorizontalAlignment="Left" VerticalAlignment="Top" Height="40" 
            Margin="50,58,0,0"  Width="40" Source="Picture1.png" Stretch="Fill" />
        <Image x:Name="testImage3" HorizontalAlignment="Left" VerticalAlignment="Top" Height="40" 
            Margin="355,143,0,0"  Width="40" Source="Picture1.png" Stretch="Fill" RenderTransformOrigin="-5.625,-2.475" />
    </Grid>
</Window>
